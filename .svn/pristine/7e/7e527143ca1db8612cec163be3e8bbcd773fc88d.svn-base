	<select id="selectUserDev" parameterType="java.lang.String"
		resultMap="DevInfoAccMapper">
		SELECT
		di.* ,ud.own_type, da.is_online ,da.last_time
		FROM
		t_dev_info di,
		t_dev_account da,
		t_pow_user_dev ud
		WHERE
		di.device_id =
		da.device_id
		AND
		di.device_id =
		ud.device_id
		AND ud.client_id
		=#{clientId,jdbcType=VARCHAR}
	</select>
	<select id="selectDevUser" parameterType="java.lang.String"
		resultMap="DevClientUser">
		SELECT
		cu.*,pd.own_type
		FROM
		t_pow_user_dev pd,
		t_pow_client_user cu
		WHERE
		pd.client_id = cu.client_id
		AND pd.device_id =
		#{deviceId,jdbcType=VARCHAR}
	</select>

