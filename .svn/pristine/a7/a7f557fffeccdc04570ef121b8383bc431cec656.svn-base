                PUBLIC CO_TKEN0
                PUBLIC CO_TKEN1
                PUBLIC CO_TKEN2
                PUBLIC CO_TKC1
                PUBLIC CO_TKC2
                PUBLIC CO_MODE
				PUBLIC CO_MARK
                PUBLIC CO_DEBOUNCE
