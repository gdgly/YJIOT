    DB    0x0C,0x0C,0x0C,0x0C,0x0C,0x0C,0x0C,0x0C
	CSEG   AT   1F00H
CO_OFFSET:
	DW	  0xFFFF,0xFFFF,0xFFFF,0xFFFF,0xFFFF,0xFFFF
	DW	  0xFFFF,0xFFFF,0xFFFF,0xFFFF,0xFFFF,0xFFFF
    DW    0xFFFF,0xFFFF,0xFFFF,0xFFFF,0xFFFF,0xFFFF
    DW    0xFFFF,0xFFFF,0xFFFF,0xFFFF,0xFFFF,0xFFFF
