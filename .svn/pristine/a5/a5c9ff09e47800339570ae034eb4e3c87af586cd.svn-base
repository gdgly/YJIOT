package com.yjyz.iot.exam.dto;

import java.util.List;

import com.yjyz.iot.device.dto.DevCommDto;
import com.yjyz.iot.exam.entity.DevExamDetail;

public class DevExamDto extends DevCommDto {
	private String type;
	private String examId;
	private Boolean isPass;
	private String memo;
	
	private List<DevExamDetail> examItems;

	public List<DevExamDetail> getExamItems() {
		return examItems;
	}

	public void setExamItems(List<DevExamDetail> examItems) {
		this.examItems = examItems;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getExamId() {
		return examId;
	}

	public void setExamId(String examId) {
		this.examId = examId;
	}

	public Boolean getIsPass() {
		return isPass;
	}

	public void setIsPass(Boolean isPass) {
		this.isPass = isPass;
	}

	public String getMemo() {
		return memo;
	}

	public void setMemo(String memo) {
		this.memo = memo;
	}

}
