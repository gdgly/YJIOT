<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yjyz.iot.security.dao.PowRoleResourceMapper">
	<resultMap id="BaseResultMap"
		type="com.yjyz.iot.security.entity.PowRoleResourceKey">
		<id column="role_id" jdbcType="VARCHAR" property="roleId" />
		<id column="res_id" jdbcType="VARCHAR" property="resId" />
	</resultMap>
	<delete id="deleteByPrimaryKey" parameterType="com.yjyz.iot.security.entity.PowRoleResourceKey">
		delete from
		t_pow_role_resource
		where role_id = #{roleId,jdbcType=VARCHAR}
		and
		res_id = #{resId,jdbcType=VARCHAR}
	</delete>

	
	<select id="selectRoleResource" parameterType="com.yjyz.iot.security.entity.PowRoleResourceKey"
		resultType="com.yjyz.iot.security.entity.PowResource">
		select
		pr.*
		from t_pow_role_resource
		prr,t_pow_resource pr
		where ppr.role_id = pr.role_id and
		pr.role_id = #{roleId,jdbcType=VARCHAR}
	</select>

	<insert id="insert" parameterType="com.yjyz.iot.security.entity.PowRoleResourceKey">
		insert into
		t_pow_role_resource (role_id, res_id)
		values
		(#{roleId,jdbcType=VARCHAR}, #{resId,jdbcType=VARCHAR})
	</insert>
	<insert id="insertSelective" parameterType="com.yjyz.iot.security.entity.PowRoleResourceKey">
		insert into t_pow_role_resource
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="roleId != null">
				role_id,
			</if>
			<if test="resId != null">
				res_id,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="roleId != null">
				#{roleId,jdbcType=VARCHAR},
			</if>
			<if test="resId != null">
				#{resId,jdbcType=VARCHAR},
			</if>
		</trim>
	</insert>
</mapper>